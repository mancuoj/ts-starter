{
  "name": "pkg-name",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@8.15.4",
  "description": "pkg-desc",
  "author": {
    "name": "mancuoj",
    "url": "https://github.com/mancuoj"
  },
  "license": "MIT",
  "homepage": "https://github.com/mancuoj/pkg-name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mancuoj/pkg-name.git"
  },
  "bugs": "https://github.com/mancuoj/pkg-name/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bunchee",
    "dev": "bunchee -w",
    "lint": "eslint .",
    "test": "vitest",
    "typecheck": "tsc",
    "prepare": "simple-git-hooks",
    "prepublishOnly": "pnpm build",
    "release": "bumpp && pnpm publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.0",
    "@types/node": "^20.11.25",
    "bumpp": "^9.4.0",
    "bunchee": "^4.4.8",
    "eslint": "^8.57.0",
    "lint-staged": "^15.2.2",
    "rimraf": "^5.0.5",
    "simple-git-hooks": "^2.10.0",
    "typescript": "^5.4.2",
    "vitest": "^1.3.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
